{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "*.js",
      "*.css",
      "*.svg",
      "*.png",
      "*.jpg",
      "*.ico",
      "/api/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
